{
	"env": {
		"node": true,
		"mocha": true,
		"jasmine": true,
		"browser": false
	},
    "rules": {
        "ident": [0, 4],
		"semi": [ 2, "always"],
		"quotes": [0, "single"],
        "linebreak-style": [0, "unix"],
		
		"no-unused-vars": [2, { "args": "none" }],
		"no-mixed-spaces-and-tabs": 0,
		"no-console": 0,
		
		"comma-dangle": 0
    },
    "extends": "eslint:recommended"
}